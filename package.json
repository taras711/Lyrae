{
  "dependencies": {
    "jsonwebtoken": "^9.0.2"
  },
  "devDependencies": {
    "jest": "^29.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext .js,.jsx",
    "format": "prettier --write .",
    "test:scheduler": "jest tests/scheduler/IntentScheduler.spec.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "\\.(jpg|jpeg|png|gif|svg)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "coverageReporters": [
      "json",
      "text",
      "lcov"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/src/setupTests.js",
      "/src/index.js",
      "/src/serviceWorker.js",
      "/src/reportWebVitals.js"
    ]
  },
  "name": "lyrae",
  "version": "1.0.0",
  "description": "Lyrae - Adaptive Scenario Router and Security Management",
  "author": "Taras",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/lyrae.git" 
  },
  "keywords": [
    "react",
    "adaptive",
    "scenario",
    "router",
    "security",
    "management",
    "audit",
    "dynamic",
    "rules",
    "modular",
    "react-app"
  ],
  "homepage": "https://yourusername.github.io/lyrae",
  "bugs": {
    "url": "https://github.com/yourusername/lyrae/issues"
  },
  "config": {
    "maxExportSize": 50,
    "allowGuestUpload": true,
    "securityOverrides": {
      "auditor": true,
      "guest": false
    }
  }
}
